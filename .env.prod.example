# Variables d'environnement pour PROD (example)
# Copiez ce fichier vers .env.prod et ajustez selon vos besoins

# DEBUG should be False for PROD
DEBUG=False

# Django settings for PROD
DJANGO_SECRET_KEY=your-prod-secret-key-here
HOST=velutina.ovh
DEV_HOST=dev.velutina.ovh

# Database settings for PROD
DB_PASSWORD=your-prod-db-password-here

# Keycloak settings for PROD  
KEYCLOAK_DB_PASSWORD=your-prod-keycloak-db-password-here
KC_CLIENT_ID=hornet-finder-prod
KC_CLIENT_SECRET=your-prod-client-secret-here
KC_BOOTSTRAP_ADMIN_PASSWORD=your-prod-admin-password-here
KC_HOSTNAME=auth.velutina.ovh

# Network settings for PROD
PROD_IP=your-prod-server-ip-here  # IP publique du serveur PROD
PROXY_PORT=80                     # Port 80 pour PROD
# Port 443 n'est pas configurable (toujours 443 pour HTTPS)

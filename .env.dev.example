# Variables d'environnement pour DEV (example)
# Copiez ce fichier vers .env.dev et ajustez selon vos besoins

# DEBUG should be True for DEV
DEBUG=True

# Django settings
DJANGO_SECRET_KEY=your-dev-secret-key-here
HOST=dev.velutina.ovh

# Database settings
DB_PASSWORD=your-dev-db-password-here

# Keycloak settings
KEYCLOAK_DB_PASSWORD=your-dev-keycloak-db-password-here
KC_CLIENT_ID=hornet-api-dev
KC_CLIENT_SECRET=your-dev-client-secret-here
KC_BOOTSTRAP_ADMIN_PASSWORD=your-dev-admin-password-here
KC_HOSTNAME=auth.dev.velutina.ovh

# Network settings
BIND_IP=your-dev-server-ip-here  # IP publique du serveur
PROXY_PORT=80                    # Port HTTP
PROXY_SSL_PORT=443               # Port HTTPS

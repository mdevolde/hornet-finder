# Variables d'environnement pour DEV (example)
# Copiez ce fichier vers .env.dev et ajustez selon vos besoins

# DEBUG should be True for DEV
DEBUG=True

# Django settings for DEV
DEV_DJANGO_SECRET_KEY=your-dev-secret-key-here
HOST=dev.velutina.ovh
DEV_HOST=dev.velutina.ovh

# Database settings for DEV
DEV_DB_PASSWORD=your-dev-db-password-here

# Keycloak settings for DEV  
DEV_KEYCLOAK_DB_PASSWORD=your-dev-keycloak-db-password-here
DEV_KC_CLIENT_ID=hornet-finder-dev
DEV_KC_CLIENT_SECRET=your-dev-client-secret-here
DEV_KC_BOOTSTRAP_ADMIN_PASSWORD=your-dev-admin-password-here

# Network settings for DEV
DEV_IP=your-dev-server-ip-here  # IP publique du serveur DEV
DEV_PROXY_PORT=80               # Port 80 pour DEV (car IP différente)
DEV_PROXY_SSL_PORT=443          # Port 443 pour DEV (car IP différente)
